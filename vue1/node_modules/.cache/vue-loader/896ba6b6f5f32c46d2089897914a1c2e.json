request.get(\"/user/count\").then(res => {\n        if (res.code === '0') {\n          res.data.forEach(item => {\n            option.series[0].data.push({name: item.address, value: item.count})\n          })\n          // 绘制图表\n          myChart.setOption(option);\n        }\n      })\n\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"]}]}