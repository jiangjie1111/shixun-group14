end: {\n          orient: 'vertical',\n          trigger: 'item',\n          left: 'center'\n        },\n        toolbox: {\n          show: true,\n          feature: {\n            mark: {show: true},\n            dataView: {show: true, readOnly: false},\n            restore: {show: true},\n            saveAsImage: {show: true}\n          }\n        },\n        series: [\n          {\n            name: '用户比例',\n            type: 'pie',\n            radius: [50, 150],\n            center: ['50%', '60%'],\n            roseType: 'area',\n            itemStyle: {\n              borderRadius: 8\n            },\n            data: []\n          }\n        ]\n      }\n      request.get(\"/user/count\").then(res => {\n        if (res.code === '0') {\n          res.data.forEach(item => {\n            option.series[0].data.push({name: item.address, value: item.count})\n          })\n          // 绘制图表\n          myChart.setOption(option);\n        }\n      })\n\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":""}]}